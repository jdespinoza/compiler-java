J ::=   J:j TK_MAS T:t 
        {:
          BinaryExpression oper = new BinaryExpression();
          oper.setSuma( j , t );
          RESULT = oper;
        :}
        | J:j TK_MENOS T:t 
        {:
          BinaryExpression oper = new BinaryExpression();
          oper.setResta( j , t );
          RESULT = oper;
        :}
        | T:t
        {:
          RESULT = t;
        :};
      
T ::=   T:t TK_POR F:f 
        {:
          BinaryExpression oper = new BinaryExpression();
          oper.setMulti( t , f );
          RESULT = oper;
        :}
      | T:t TK_DIV F:f 
        {:
          BinaryExpression oper = new BinaryExpression();
          oper.setDiv( t , f );
          RESULT = oper;
        :}
      | T:t TK_GREATER F:f
        {:
          BinaryExpression oper = new BinaryExpression();
          oper.setGreater( t, f );
          RESULT = oper;
        :}
      | T:t TK_LESS F:f
        {:
          BinaryExpression oper = new BinaryExpression();
          oper.setLess( t, f );
          RESULT = oper;
        :}
      | T:t TK_EQUAL F:f
        {:
          BinaryExpression oper = new BinaryExpression();
          oper.setEqual( t, f );
          RESULT = oper;
        :}
      | T:t TK_DIFFERENT F:f
        {:
          BinaryExpression oper = new BinaryExpression();
          oper.setDifferent( t, f );
          RESULT = oper;
        :}
      | T:t TK_AND F:f
        {:
          BinaryExpression oper = new BinaryExpression();
          oper.setAND( t, f );
          RESULT = oper;
        :}
      | T:t TK_OR F:f
        {:
          BinaryExpression oper = new BinaryExpression();
          oper.setOR( t, f );
          RESULT = oper;
        :}
      | F:f 
        {:
          RESULT = f;
        :};
      
F ::= TK_NUM:num 
      {:
        LiteralExpression numero = new LiteralExpression();
        numero.setLiteral( num );
        RESULT = numero;
      :}
    | TK_ID:id
      {:
        LiteralExpression numero = new LiteralExpression();
        numero.setLiteral( table.getValue(id) );
        RESULT = numero;
      :}
    | TK_LPAREN J:j TK_RPAREN 
      {:
        j.setParentesis(true);
        RESULT = j;
      :};
      